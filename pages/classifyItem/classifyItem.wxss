/*底部  */
.gwc-mask {
  z-index: 999;
}
.footer {
  position: fixed;
  left: 0;
  bottom: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 98rpx;
  background: #fff;
  z-index: 9999;
}
.footer .sel-con-tip {
  display: flex;
  align-items: center;
  flex: 1;
  height: 98rpx;
  color: #000000;
  font-family: PingFangSC-Regular;
  font-size: 14px;
}
.footer .sel-con-tip .gwc {
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 68rpx;
  height: 69rpx;
  margin: 0 22rpx;
  border-radius: 50%;
}
.footer .sel-con-tip .no-gwc {
  background: #ADADAD;
}
.footer .sel-nums {
  position: absolute;
  right: -20rpx;
  top: -4rpx;
  width: 32rpx;
  height: 32rpx;
  border: 1px solid #FFFFFF;
  border-radius: 50%;
  text-align: center;
  line-height: 32rpx;
  background: #FF972C;
}

.footer .sel-con-tip image {
  width: 36rpx;
  height: 36rpx;
}
.footer .share-btn {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 240rpx;
  height: 98rpx;
  color: #fff;
  text-align: center;
  line-height: 14px;  
  font-family: PingFangSC-Regular;
  font-size: 16px;
}
.no-share-btn {
  background: #ADADAD;
}


/*购物车弹窗  */
.gwc-con {
  position: absolute;
  left: 0;
  bottom: 98rpx;
  width: 100%;
  background: #fff;
  z-index: 9999;
  transition: all 2s;
  -ms-transition: all 2s;
  -moz-transition: all 2s;
  -webkit-transition: all 2s;
  transform: translateY(98rpx);
  -ms-transform: translateY(98rpx);
  -moz-transform: translateY(98rpx);
  -webkit-transform: translateY(98rpx);
  -o-transform: translateY(98rpx);
}
.gwc-tit {
  display: flex;
  align-items: center;
  padding: 0 30rpx;
  height: 82rpx;
  background: #F5F5F5;
}
.gwc-tit icon {
  position: absolute;
  top: 0;
  right: 28rpx;
  line-height: 80rpx;
  font-size: 18px;
}

.gwc-tit text {
  opacity: 0.5;
  font-family: PingFangSC-Regular;
  font-size: 16px;
  color: #000000;
  text-align: left;
  line-height: 16px;  
}
.gwc-list-con {
  height: 600rpx;
  /* padding-bottom: 50rpx; */
}
.gwc-list {
  display: flex;
  align-items: center;
  height: 65px;
  margin: 0 30rpx;
}
.gwc-list .tits {
  width: 210px;
  margin-right: 64rpx;
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #000000;
  text-align: left;
  line-height: 18px;  
}
.gwc-list .money {
  flex: 1;
  /* margin-right: 84rpx; */
  font-family: PingFangSC-Regular;
  font-size: 16px;
  color: #FF972C;
  line-height: 16px;
}
.gwc-list image {
  width: 34rpx;
  height: 40rpx;
}

.sel-gwc-con {
  transition: all 2s;
  -ms-transition: all 2s;
  -moz-transition: all 2s;
  -webkit-transition: all 2s;
  -o-transition: all 2s;
  transform: translateY(0);
  -ms-transform: translateY(0);
  -moz-transform: translateY(0);
  -webkit-transform: translateY(0);
  -o-transform: translateY(0);
}
.sel-mask-bg {
  opacity: 1;
  transition: all 2s;
  -ms-transition: all 2s;
  -moz-transition: all 2s;
  -webkit-transition: all 2s;
  -o-transition: all 2s;
}