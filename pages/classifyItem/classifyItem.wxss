/*头部搜索  */
.header-con {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  background: #fff;
  z-index: 999;
  transition: all 0.5s;
  -ms-transition: all 0.5s;
  -moz-transition: all 0.5s;
  -webkit-transition: all 0.5s;
  -o-transition: all 0.5s;
}

.header {
  width: 100%;
  padding: 20rpx 0;
}
.header_display {
  transition: all 0.5s;
  -ms-transition: all 0.5s;
  -moz-transition: all 0.5s;
  -webkit-transition: all 0.5s;
  -o-transition: all 0.5s;
  transform: translateY(-50px);
  -ms-transform: translateY(-50px);
  -moz-transform: translateY(-50px);
  -webkit-transform: translateY(-50px);
  -o-transform: translateY(-50px);
}

.header > .search {
  height: 60rpx;
  line-height: 60rpx;
  background: #FFF;
  margin: 0 30rpx 0;
  border-radius: 100px;
  text-align: center;
  font-size: 0;
  background: #F1F1F1;
}

.header > .search > .icon {
  vertical-align: middle;
  margin-right: 10rpx;
  width: 28rpx;
}

.header > .search > text {
  vertical-align: middle;
  opacity: 0.5;
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #000000;
}

.header > .ser-promise {
  display: flex;
  align-items: center;
  margin: 0 22rpx;
  height: 80rpx;
}

.header > .ser-promise > .item {
  flex: 1;
  font-size: 0;
}

.header > .ser-promise > .item > .icon {
  margin-right: 12rpx;
  vertical-align: middle;
  width: 28rpx;
}

.header > .ser-promise > .item > text {
  vertical-align: middle;
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #FFFFFF;
  line-height: 12px;
}




/*顶部栏  */
.top-menus-tit {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 80rpx;
  background: #fff;
}
.top-menus-tit .item {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  line-height: 40px;
  font-family: 'PingFangSC-Regular';
  font-size: 14px;
}
.top-menus-tit .item icon {
  margin-left: 6px;
  line-height: 40px;
  font-size: 7px;
}
.top-menus-tit .item .all_icon {
  transform: rotate(180deg);
  -ms-transform:rotate(180deg);
  -moz-transform:rotate(180deg);
  -webkit-transform:rotate(180deg);
  -o-transform:rotate(180deg); 	
}
.top-menus-tit .item image {
  width: 6px;
  height: 10px;
  margin-left: 16px;
}

/*热门  */
.mask {
  position: fixed;
  left: 0;
  top: 99px;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.6);
  z-index: 99999;
}
.mask .hot {
  background: #fff;
}
.mask .hot .hot-con {
  overflow-y: scroll;
  height: 259px;
  padding: 20px 15px 50px 15px;
}
.mask .hot .hot-tit {
  margin-bottom: 18px;
  color: #1C1C1C;
  line-height: 16px;
  font-family: 'PingFangSC-Regular';
  font-size: 14px;
}
.mask .hot-cons {
  overflow: hidden;
}
.mask .hot-cons- {
  width: 110%;
}
.mask .hot-cons .item {
  position: relative;
  float: left;
  width: 216rpx;
  height: 90rpx;
  margin-right: 15rpx;
  margin-bottom: 18rpx;
  border: 1px solid #F7F7F8; 
  border-radius: 4rpx;
  color: #666666;
  text-align: center;
  line-height: 90rpx;
  font-family: 'PingFangSC-Regular';
  font-size: 13px;
  background: #F7F7F8;
}
.mask .hot-cons .sel-img {
  position: absolute;
  right: -2rpx;
  bottom: -2rpx;
  width: 16px;
  height: 16px;
}
.mask .hot-cons .item-sel {
  color: var(--themeBg);
  border: 1px solid var(--themeBg); 
  border-radius: 2px;
  background: #DEECFF;
}
.mask .btn {
  display: flex;
  justify-content: center;
  align-items: center;
}
.mask .btn view {
  width: 50%;
  height: 47px;
  text-align: center;
  line-height: 47px;
  font-size: 16px;
  font-family: 'PingFangSC-Regular';
}
.mask .btn .cancel {
  background: #FFFFFF;
  box-shadow: 0 0 3px 0 rgba(0,0,0,0.14);
}

/*热门-目的地-列表  */
.commoduty {
  padding-top: 180rpx;
  padding-bottom: 40rpx;  
  background: #fff;
}
.commoduty .container {
  margin: 0 15px;
}
.commoduty .list {
  display: flex;
  padding: 24rpx 0;
}
.reduce {
  padding: 2rpx 16rpx;
  font-size: 12px;
  color: #FFA816;
  border: 1px solid #FFA816;
  line-height: 24rpx;
  height: 24rpx;
  border-radius: 100px;
}

.sel-btn-con {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 56rpx;
  height: 220rpx;
}
.sel-btn-con image {
  width: 36rpx;
  height: 36rpx;
  margin-left: -26rpx;
}

.commoduty .pic-discr {
  position: relative;
  margin-right: 9px;
  border-radius: 4rpx;
  overflow: hidden;
  box-shadow: 0 2px 4px 0 rgba(0,0,0,0.29);
}

.pic-discr image {
  display: block;
  width: 220rpx;
  height: 220rpx;
}
.pic-discr text {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 20px;
  color: #FFFFFF;
  text-align: center;
  line-height: 20px;
  font-family: 'PingFangSC-Regular';
  font-size: 12px;
  background: rgba(0, 0, 0, 0.5);
}

.info-con {
  flex: 1;
  display: flex;
  align-items: center;
  height: 110px;
}
.info-con .info-tit {
  height: 36px;
  margin-bottom: 6px;
  color: #000000;
  text-align: left;
  line-height: 18px;
  font-family: 'PingFangSC-Regular';
  font-size: 14px;
}
.info-con .info-time {
  margin-bottom: 21px;
  color: #ADADAD;
  line-height: 24px;
  font-family: 'PingFangSC-Regular';
  font-size: 12px;
}
.info-price .price {
  margin-right: 8px;
  color: #333333;
  line-height: 36px;
  font-family: 'PingFangSC-Medium';
  font-size: 18px;
}
.info-price .shop-price {
  font-size: 16px;
}

.no-more {
  padding-bottom: 40rpx;
  text-align: center;
}

.no-more > .line {
  display: inline-block;
  vertical-align: middle;
  width: 80rpx;
  height: 1px;
  background: #AAA;
}

.no-more > .text {
  margin: 0 30rpx;
  vertical-align: middle;
  font-size: 14px;
  line-height: 60rpx;
  color: #AAA;
}

.no {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-top: 300rpx;
}

.no > .icon {
  margin-bottom: 30rpx;
  width: 300rpx;
}

.no > .text {
  font-family: PingFangSC-Light;
  font-size: 14px;
  color: #666666;
  letter-spacing: -0.3px;
  line-height: 30rpx;
}

.no-more {
  padding-bottom: 10rpx;
  opacity: 0.3;
  font-family: PingFangSC-Regular;
  font-size: 12px;
  color: #222333;
  line-height: 90rpx;
  text-align: center;
}

/*佣金  */
.commiss {
  display: inline-flex;
  align-items: center;
  width: 100%;
}
.commiss-con {
  display: flex;
  flex: 1;
  font-size: 16px;
  font-family: 'PingFangSC-Regular';
}
.commiss-con text {
  color: #FF972C;
}

/*分享弹窗  */
.mask {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.6);
  z-index: 9999;
}
.mask-bg {
  width: 100%;
  height: 100%;
  opacity: 0;
  transition: all 0.2s;
  -ms-transition: all 0.2s;
  -moz-transition: all 0.2s;
  -webkit-transition: all 0.2s;
  -o-transition: all 0.2s;
}
.hot-mask {
  top: 88px;
  z-index: 99999;
}
.mask .share {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 456rpx;
  background: #fff; 
}
.mask .title {
  position: relative;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;  
  color: #666666;
  text-align: center;
  font-size: 12px;
  font-family: 'PingFangSC-Regular';
  background: #F5F5F5;
}
.mask .title icon {
  position: absolute;
  top: 0;
  right: 28rpx;
  line-height: 80rpx;
  font-size: 18px;
}
.mask .share-con {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 376rpx;
}
.mask .items {
  position: relative;
  margin: 0 55rpx;
}
.mask .items button {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100rpx;
  margin: 0;
  padding: 0;
  border: none;
  outline: none;
  background-color: transparent;
  z-index: 2;
}
.mask .items button:after {
  border: none;
}
.mask .items image {
  display: block;
  width: 100rpx;
  height: 100rpx;
  margin: 0 auto 24rpx;
}
.mask .items text {
  display: block;
  color: #666666;
  text-align: center;
  font-size: 12px;
  font-family: 'PingFangSC-Regular';
}

