/*分类  */
.classify {
  position: relative;
  overflow: hidden;
  margin: -36rpx 14rpx 0 14rpx;
  box-shadow:0 3px 4px 0 #ddd; 
}

.classify > .menu {
  margin: 0 auto;
  padding-top: 24rpx;
  border-radius: 2px;
  background: #fff;
}
.classify > .menu > .item {
  float: left;
  width: 25%;
  margin-bottom: 24rpx;
}
.classify > .menu > .item > .pic {
  display: block;
  width: 80rpx;
  height: 80rpx;
  margin: 0 auto 24rpx;
}
.classify > .menu > .item > text {
  display: block;
  color: #2B2C3E;
  text-align: center;
  line-height: 14px;
  font-size: 14px;
  font-family: PingFangSC-Regular;
}

