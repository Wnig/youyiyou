.part1 {
  display: flex;
  flex-direction: column;
  padding: 60rpx 0;
  align-items: center;
  background: #FFF;
}

.part1 > .icon {
  margin-bottom: 40rpx;
  width: 100rpx;
  height: 100rpx;
}

.part1 > .text {
  margin-bottom: 10rpx;
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #000000;
  letter-spacing: -0.34px;
  line-height: 16px;
}

.part1 > .btn {
  padding: 0 30rpx;
  margin-top: 40rpx;
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #FFFFFF;
  letter-spacing: -0.34px;
  border-radius: 3px;
  line-height: 80rpx;
}